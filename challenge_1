function studentGradeGenerator(marks) {
    if (marks < 0 || marks > 100) {
        return "Invalid marks. Please enter a value between 0 and 100.";
    }

    if (marks > 79) return "Grade: A";
    if (marks >= 60) return "Grade: B";
    if (marks >= 50) return "Grade: C";
    if (marks >= 40) return "Grade: D";
    return "Grade: E";
}


const marks = 85; 
console.log(studentGradeGenerator(marks));
